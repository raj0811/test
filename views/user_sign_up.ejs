                <input type="submit" id="signup-btn" value="Signup">
            </form>

           <a href="google.com" class="g-auth">
            
                google
            
           </a>
        </div>

        <div class=" txt-center">
          <a href="" class="signup-link">
            Sign-in here
          </a>
        </div>
      </div>

<script src="https://code.jquery.com/jquery-3.6.1.min.js"></script>


